    "migrate:staging": "node scripts/migrate-dev-to-staging.js"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "box-node-sdk": "^10.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "pg-copy-streams": "^7.0.0",
    "shapefile": "^0.6.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0"
  }
}
